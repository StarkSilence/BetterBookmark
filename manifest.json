  "permissions":
  [
    "tabs",
    "storage"
  ],
  "browser_specific_settings": 
  {
    "gecko": 
    {
      "id": "sharpshooter908@gmail.com",
      "strict_min_version": "42.0"
    }
  },  
  "browser_action":
  {
    "default_icon": "/icons/one-tab-96.png",
